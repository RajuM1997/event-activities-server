{
  "name": "event_activities_server",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "db:generate": "prisma generate --schema=./prisma/schema",
    "db:migrate": "prisma migrate --schema=./prisma/schema",
    "db:push": "prisma db push --schema=./prisma/schema",
    "db:pull": "prisma db pull --schema=./prisma/schema",
    "db:studio": "prisma studio --schema=./prisma/schema",
    "postinstall": "prisma generate --schema=./prisma/schema",
    "stripe:webhook": "stripe listen --forward-to localhost:5000/webhook"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "prisma": "^7.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "stripe": "^20.1.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.4"
  }
}
